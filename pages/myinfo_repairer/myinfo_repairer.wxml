<!--pages/myinfo_repairer/myinfo_repairer.wxml-->
<i-panel i-class="i-page">
  <view class="pic"><open-data type="userAvatarUrl"></open-data></view>
  <i-input title="性别：" value="{{list.sex_repairer}}" type="text" mode="wrapped" name="sex_repairer"  disabled />
  <i-input title="姓名：" value="{{list.name_repairer}}" type="number" mode="wrapped" name="name_repairer"  disabled />
  <i-input title="学号：" value="{{no_repairer}}" type="text" mode="wrapped" name="no_repairer"  disabled />
  <i-input title="账户密码：" value="{{list.pass_repairer}}" type="text" mode="wrapped" name="pass_repairer"  disabled />
  <i-input title="联系方式：" value="{{list.phone_repairer}}" type="text" mode="wrapped" name="phone_repairer"  disabled />
  <button class="btn" bindtap="change">修改信息</button>
</i-panel>
